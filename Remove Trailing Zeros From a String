

string removeTrailingZeros(string num) {
        int len ;
        for(int i=num.length()-1 ; i>=0 ; i--){
            if(num[i]!='0'){
                len = i;
                break;
            }
        }
        string str="";
        for(int i=0;i<=len;i++){
            str+=num[i];
        }
        return str;
    }
